<?xml version="1.0" encoding="UTF-8"?>
<!-- Generated by Apache ServiceMix Archetype -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:bean="http://servicemix.apache.org/bean/1.0"
       xmlns:ewaf="http://melexis.com/electronicwafermapping"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://servicemix.apache.org/bean/1.0 http://servicemix.apache.org/schema/servicemix-bean-2011.01-fuse-00-00.xsd
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


    <bean:endpoint service="ewaf:wmdb" endpoint="xml" bean="#wmdb"/>

    <bean id="attachWafermapTransformer" class="com.melexis.AttachWafermapsTransformer">
        <constructor-arg ref="waferService"/>
    </bean>

    <bean id="xmlutil" class="com.melexis.util.XmlUtil" />

    <bean id="messageUtil" class="com.melexis.util.MessageUtil" >
        <constructor-arg ref="xmlutil"/>
    </bean>

    <bean id="wmdb" class="com.melexis.util.ProcessLotExchangeListener">
        <constructor-arg ref="attachWafermapTransformer"/>
        <constructor-arg ref="messageUtil"/>
    </bean>
    
    <import resource="classpath:/beans.xml" />

</beans>
